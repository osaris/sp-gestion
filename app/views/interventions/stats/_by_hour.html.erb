<div class="row">
  <div class="col-md-9">
    <%= high_chart("chart", @chart) %>
  </div>
  <div class="col-md-3">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Heure</th>
          <th>Nombre</th>
        </tr>
      </thead>
      <tbody>
      <%- data.each_with_index do |number, hour| -%>
        <tr>
          <td><%= hour %></td>
          <td><%= number %></td>
        </tr>
      <%- end -%>
      </tbody>
    </table>
  </div>
</div>
